{
  "name": "solana-radar-agent",
  "version": "1.0.0",
  "description": "Autonomous AI agent that detects emerging Solana ecosystem narratives from on-chain, social, developer, and research signals",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "collect": "node -e \"require('dotenv').config(); require('./cron/scheduler').runCollection()\"",
    "analyze": "node -e \"require('dotenv').config(); require('./cron/scheduler').runAnalysis()\"",
    "full-run": "node -e \"require('dotenv').config(); require('./cron/scheduler').runFull()\""
  },
  "keywords": [
    "solana",
    "narrative",
    "radar",
    "agent",
    "ai",
    "signals",
    "trends",
    "superteam"
  ],
  "author": "Xona Labs",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
